    "def pythag_triplet():\n",
    "    for a in range(1,500):\n",
    "        for b in range(1,500):\n",
    "            c = (a**2 + b**2)**(1/2)\n",
    "            if (c + a + b) == 1000:\n",
    "                return int(a*b*c)\n",
    "            elif (c + a + b) > 1000:\n",
    "                break\n",
    "print(pythag_triplet())"
   ]
